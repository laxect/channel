{
  "incremental": true,
  "exec": {
    "associations": "**/*.scm",
    "timeout": 30,
    "guix_style": "./guix-style-wrapper.sh {{file_path}}",
    "guix_style.stdin": false,
    "guix_style.associations": "**/*.scm"
  },
  "json": {},
  "markdown": {},
  "includes": [
    "**/*.{json,md,scm}"
  ],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/json-0.15.5.wasm",
    "https://plugins.dprint.dev/markdown-0.14.0.wasm",
    "https://plugins.dprint.dev/exec-0.3.1.json@9351b67ec7a6b58a69201c2834cba38cb3d191080aefc6422fb1320f03c8fc4d"
  ]
}
